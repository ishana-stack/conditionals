print("select your ride")
print("1.bike ")
print("2.car")
choice=int(raw_input("enter your choice"))
if choice==1:
    print("what type of bike")
    print("1.scooty")
    print("2.scooter")
    choice2=int(raw_input("enter your choice"))
    if choice2==1:
        print("you have selected scooty")
    else:
        print("you have selected scooter")
elif choice==2:
    print("what type of car")
    print("1.cedan")
    print("2.suv")
    choice3=int(raw_input("enter your choice"))
    if choice3==1:
        print("you have selected a cedan")
    else:
        print("you have selected a suv")
else:
    print("wrong choice")
    
