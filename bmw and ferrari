        "class vehicle():\n",
        "  def__init__(self,speed,colour,ac):\n",
        "    self.speed=speed\n",
        "    self.colour=colour\n",
        "    self.ac=ac\n",
        "class bmw(vehicle):\n",
        "  def__init__(self,speed,colour,superac,style):\n",
        "    pass\n",
        "    self.superac=superac\n",
        "    self.style=style\n",
